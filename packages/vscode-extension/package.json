{
  "publisher": "brojor",
  "name": "devlog",
  "displayName": "DevLog",
  "version": "0.1.0",
  "private": true,
  "description": "Automatic time tracking and development logging solution",
  "repository": {
    "type": "git",
    "url": "https://github.com/brojor/devlog.git"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "devlog.togglePause",
        "title": "DevLog: Toggle Pause"
      }
    ],
    "configuration": {
      "title": "DevLog",
      "properties": {
        "devlog.serverUrl": {
          "type": "string",
          "default": "http://n40l:3000",
          "description": "URL adresa pro DevLog server"
        }
      }
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint --fix src --ext ts",
    "package": "vite build && vsce package --no-dependencies --out dist"
  },
  "dependencies": {
    "@devlog/shared": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/vscode": "^1.96.0",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
